.loginWrapper {
  background-color: #4c505f;
  /* I don't think this color is correct in the spec */
  color: #B5B9C8;
  height: 800px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.headWrap {
  text-align: center;
  font-weight: 100;
}

.fw100 {
  font-weight: 100;
}

.loginCard {
  padding-top: 2rem;
  background-color: #585D6C;
  margin-top: 6rem;
  color: #B5B9C8;
  padding-right: 2rem;
  width: 50rem;
  height: 30rem;
  box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);

}

.loginButton {
  background-color: #68C6BE;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50rem;
  height: 8rem;
  margin-top: 50px;
  cursor: pointer;
}

.createButton {
  background-color: rgba(87, 128, 11, 0.98);
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50rem;
  height: 8rem;
  margin-top: 50px;
  cursor: pointer;
}

.buttonWrap {
  padding-top: 25px;
  cursor: pointer;
}
