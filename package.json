    "meteor-node-stubs": "~0.2.0",
    "normalize.css": "4.1.1",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "15.0.1",
    "react-addons-shallow-compare": "15.0.1",
    "react-dom": "^15.0.0",
    "react-helmet": "3.0.1",
    "react-komposer": "^1.8.0",
    "react-mounter": "^1.2.0",
    "react-simple-di": "^1.2.0",
    "react-toolbox": "^0.16.2",
    "react-virtualized": "6.2.1",
    "yup": "0.18.2"
  },
  "cssModules": {
    "extensions": [
